{
    "common":{
        "LAYOUT": {
            "display": ["block", "inline", "inline-block", "flex", "grid"],
            "visibility": ["visible", "hidden"],
            "overflow": ["visible", "hidden", "auto"],
            "box-sizing": ["content-box", "border-box"],
            "margin": ["0", "auto", "10px", "1em"],
            "padding": ["0", "5px", "1em"]
        },
        "TYPOGRAPHY": {
            "font-size": ["12px", "16px", "1em", "larger", "smaller"],
            "font-weight": ["normal", "bold", "lighter", "bolder", "100", "900"],
            "text-align": ["left", "right", "center", "justify"],
            "line-height": ["1", "1.5", "2"],
            "color": ["black", "gray", "red", "#333"],
            "font-family": ["Arial", "sans-serif", "monospace"]
        },
        "BOX": {
            "background-color": ["white", "#f0f0f0", "lightblue", "transparent"],
            "border": ["none", "1px solid black", "2px dashed gray"],
            "border-radius": ["0", "4px", "10px"],
            "box-shadow": ["none", "2px 2px 5px rgba(0,0,0,0.3)"]
